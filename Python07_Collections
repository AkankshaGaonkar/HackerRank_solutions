# print(sales)

# Option without Counter
X = int(input())
sizes_list = list(map(int, input().split()))
N = int(input())
sales = 0
for _ in range(N):
    size, price = map(int, input().split())
    if size in sizes_list:
        sales += price
        sizes_list.remove(size)
print(sales)









